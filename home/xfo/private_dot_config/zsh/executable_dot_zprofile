source ~/.profile

# Data directory should be created manually since zsh doesn't conform to
# XDG Base Directory specification.
if  [[ ! -d "$HOME/.local/share/zsh" ]]; then
    command mkdir -p "$HOME/.local/share/zsh"
fi

